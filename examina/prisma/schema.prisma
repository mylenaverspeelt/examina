model Storage {
  id       Int       @id @default(autoincrement())
  fileName String
  pdf      Bytes

  Glucoses Glucose[] 
}

model Patient {
  id        Int       @id @default(autoincrement())
  name      String
  age       String
  birthDate String

  Glucoses  Glucose[] 
}

model Glucose {
  id         Int       @id @default(autoincrement())
  patientId  Int
  pdfId      Int
  result     Int
  createdAt  DateTime  @default(now())

  Patient    Patient   @relation(fields: [patientId], references: [id])
  Storage    Storage   @relation(fields: [pdfId], references: [id], onDelete: Cascade)
  @@index([patientId])
  @@index([pdfId])
}

datasource db {
  provider = "postgresql"
  url      = env("DATABASE_URL") 
}

generator client {
  provider = "prisma-client-js"
}